@model EduKeeper.Entities.Error

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1>Error!</h1>
        <h2>@(String.IsNullOrEmpty(Model.ErrorDescription) ? "An error occurred while processing your request." : Model.ErrorDescription)</h2>
        <h2>@(String.IsNullOrEmpty(Model.RedirectActionName) ? "An error occurred while processing your request." : Model.RedirectActionName)</h2>
        <h2>@(String.IsNullOrEmpty(Model.ErrorActionName) ? "An error occurred while processing your request." : Model.ErrorActionName)</h2>
    </hgroup>
</body>
</html>